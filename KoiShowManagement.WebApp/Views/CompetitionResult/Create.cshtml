@model KoiShowManagement.Repositories.Entities.CompetitionResult

@{
    ViewData["Title"] = "Tạo Kết Quả Cuộc Thi";
}

<h2>Tạo Kết Quả Cuộc Thi</h2>

<form method="post">
    <div class="form-group">
        <label for="CompetitionName">Tên Cuộc Thi</label>
        <input type="text" id="CompetitionName" name="CompetitionName" class="form-control" required />
    </div>
    <div class="form-group">
        <label for="Date">Ngày</label>
        <input type="date" id="Date" name="Date" class="form-control" required />
    </div>
    <div class="form-group">
        <label for="Details">Chi Tiết</label>
        <textarea id="Details" name="Details" class="form-control" required></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Tạo</button>
</form>
